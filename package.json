{
  "name": "digitizing-health-center",
  "version": "1.0.0",
  "description": "MEAN stack application for digitizing health center appointments and records",
  "main": "backend/server.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "node backend/server.js",
    "client": "cd frontend && npm start",
